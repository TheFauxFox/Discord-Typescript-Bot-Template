{
    "name": "discordtsbot",
    "version": "1.0.0",
    "description": "Taking over the world!",
    "main": "index.js",
    "scripts": {
        ".": "ts-node ./src/index.ts",
        "watch": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node ./src/index.ts'",
        "eslint": "eslint --fix src/**",
        "prettify": "prettier -c src/**",
        "lint": "npm run prettify && npm run eslint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TheFauxFox/Discord-Typescript-Bot-Template.git"
    },
    "author": "TheFauxFox",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TheFauxFox/Discord-Typescript-Bot-Template/issues"
    },
    "homepage": "https://github.com/TheFauxFox/Discord-Typescript-Bot-Template#readme",
    "dependencies": {
        "discord.js": "^14.6.0",
        "dotenv": "^16.0.3",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@types/node": "^18.11.9",
        "eslint": "^8.26.0",
        "prettier": "^2.7.1"
    }
}
